#: ../applets/wncklet/org.ukui.panel.Wncklet.ukui-panel-applet.in.in.h:6
msgid "Switch between workspaces"
msgstr "在工作区之间切换"

#: ../applets/wncklet/org.ukui.panel.Wncklet.ukui-panel-applet.in.in.h:7
#: ../applets/wncklet/window-list.c:598
msgid "Window List"
msgstr "窗口列表"

#: ../applets/wncklet/org.ukui.panel.Wncklet.ukui-panel-applet.in.in.h:8
msgid "Switch between open windows using buttons"
msgstr "通过按钮在打开的窗口间切换"

